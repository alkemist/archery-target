{
  "name": "archery-target",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config -- --environment=dev && ng serve",
    "start-ssl": "npm run start -- --ssl",
    "serve": "npm run start -- --no-live-reload",
    "start-live": "npm run start -- --host 0.0.0.0 --disable-host-check",
    "serve-live": "npm run serve -- --host 0.0.0.0 --disable-host-check",
    "build": "npm run config -- --environment=prod && ng build  --configuration production --base-href /",
    "build-watch": "npm run build -- --watch",
    "watch": "ng build --configuration development --watch",
    "test": "jest",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn install",
    "netlify": "netlify dev",
    "config": "ts-node ./scripts/setenv.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@angular/service-worker": "^16.0.0",
    "@ngxs/logger-plugin": "^3.8.0",
    "@ngxs/storage-plugin": "^3.8.0",
    "@ngxs/store": "^3.8.0",
    "dotenv": "^16.0.3",
    "firebase": "^9.17.1",
    "fork-awesome": "^1.2.0",
    "hammerjs": "^2.0.8",
    "ngx-valdemort": "^10.0.1",
    "nosleep.js": "^0.12.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "15.4.1",
    "rxjs": "~7.8.0",
    "stackdriver-errors-js": "^0.12.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "16.0.0",
    "@angular/compiler-cli": "^16.0.2",
    "@angular/localize": "^16.0.2",
    "@netlify/functions": "^1.0.0",
    "@ngxs/devtools-plugin": "^3.8.0",
    "@types/hammerjs": "^2.0.41",
    "@types/jest": "^27.4.0",
    "@types/node": "^20.2.3",
    "@types/xml2js": "^0.4.11",
    "es6-promise-plugin": "^4.2.2",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-preset-angular": "^13.1.1",
    "netlify-cli": "^9.13.1",
    "ng-mocks": "^14.10.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.5.0",
    "ts-patch": "^2.1.0",
    "typescript": "5.0",
    "typescript-transform-extensions": "^1.0.1",
    "webpack": "^5.76.2"
  }
}
